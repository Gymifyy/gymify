{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "resolveJsonModule": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "typeRoots": [
        "./src/types"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "gym_mock.json",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
